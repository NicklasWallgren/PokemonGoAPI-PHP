{
    "name": "nicklasw/pkm-go-api",
    "description": "The Pokemon Go API library.",
    "keywords": ["Pokemon Go API"],
    "license": "MIT",
    "type": "project",

    "require": {
        "php": ">=5.6",
        "paquettg/php-html-parser": "1.7",
        "guzzlehttp/guzzle": "~6.0",
        "vlucas/phpdotenv": "^2.3",
        "monolog/monolog": "^1.20",
        "jeremeamia/xstatic": "^1.0",
        "php-di/php-di": "^5.3",
        "phpseclib/phpseclib": "^2.0",
        "illuminate/config": "^5.2",
        "nicklasw/s2-geometry-library-php": "dev-master",
        "nicklasw/pogoprotos-php": "dev-master",
        "pogo-php/pogoencrypt-php": "^1.0",
        "paragonie/random_compat": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "NicklasW\\PkmGoApi\\": "src/"
        },
        "files": [
            "includes/config.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
